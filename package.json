{
  "name": "change-case-for-vscode",
  "version": "1.0.1",
  "displayName": "Change Case For VSCode",
  "description": "Quickly change the case (camelCase, CONSTANT_CASE, snake_case, etc) of the current selection or current word",
  "icon": "images/change-case-icon.png",
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ShiGongZhi/vscode-change-case.git"
  },
  "bugs": {
    "url": "https://github.com/ShiGongZhi/vscode-change-case/issues"
  },
  "license": "MIT",
  "publisher": "SoulFriends",
  "engines": {
    "vscode": "^1.6.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.changeCase.commands",
    "onCommand:extension.changeCase.camel",
    "onCommand:extension.changeCase.constant",
    "onCommand:extension.changeCase.dot",
    "onCommand:extension.changeCase.kebab",
    "onCommand:extension.changeCase.lower",
    "onCommand:extension.changeCase.lowerFirst",
    "onCommand:extension.changeCase.no",
    "onCommand:extension.changeCase.param",
    "onCommand:extension.changeCase.pascal",
    "onCommand:extension.changeCase.path",
    "onCommand:extension.changeCase.sentence",
    "onCommand:extension.changeCase.snake",
    "onCommand:extension.changeCase.swap",
    "onCommand:extension.changeCase.title",
    "onCommand:extension.changeCase.upper",
    "onCommand:extension.changeCase.upperFirst"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "keybindings": [
      {
        "command": "extension.changeCase.commands",
        "key": "alt+t",
        "mac": "alt+t",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.changeCase.camel",
        "key": "shift+alt+t",
        "mac": "shift+alt+t"
      }
    ],
    "commands": [
      {
        "command": "extension.changeCase.commands",
        "title": "Change Case Commands"
      },
      {
        "command": "extension.changeCase.camel",
        "title": "Change Case camel"
      },
      {
        "command": "extension.changeCase.constant",
        "title": "Change Case constant"
      },
      {
        "command": "extension.changeCase.dot",
        "title": "Change Case dot"
      },
      {
        "command": "extension.changeCase.kebab",
        "title": "Change Case kebab"
      },
      {
        "command": "extension.changeCase.lower",
        "title": "Change Case lower"
      },
      {
        "command": "extension.changeCase.lowerFirst",
        "title": "Change Case lowerFirst"
      },
      {
        "command": "extension.changeCase.no",
        "title": "Change Case no"
      },
      {
        "command": "extension.changeCase.param",
        "title": "Change Case param"
      },
      {
        "command": "extension.changeCase.pascal",
        "title": "Change Case pascal"
      },
      {
        "command": "extension.changeCase.path",
        "title": "Change Case path"
      },
      {
        "command": "extension.changeCase.sentence",
        "title": "Change Case sentence"
      },
      {
        "command": "extension.changeCase.snake",
        "title": "Change Case snake"
      },
      {
        "command": "extension.changeCase.swap",
        "title": "Change Case swap"
      },
      {
        "command": "extension.changeCase.title",
        "title": "Change Case title"
      },
      {
        "command": "extension.changeCase.upper",
        "title": "Change Case upper"
      },
      {
        "command": "extension.changeCase.upperFirst",
        "title": "Change Case upperFirst"
      }
    ],
    "configuration": {
      "properties": {
        "changeCase.includeDotInCurrentWord": {
          "type": "boolean",
          "default": false,
          "description": "When selecting current word, allow dots in current word (false by default)"
        },
        "changeCase.camel": {
          "type": "boolean",
          "default": true,
          "description": "Convert to a string with the separators denoted by having the next letter capitalised"
        },
        "changeCase.pascal": {
          "type": "boolean",
          "default": true,
          "description": "Convert to a string denoted in the same fashion as camelCase, but with the first letter also capitalised"
        },
        "changeCase.kebab": {
          "type": "boolean",
          "default": true,
          "description": "Convert to a lower case, dash separated string (alias for param case)"
        },
        "changeCase.snake": {
          "type": "boolean",
          "default": true,
          "description": "Convert to a lower case, underscore separated string"
        },
        "changeCase.lower": {
          "type": "boolean",
          "default": true,
          "description": "Convert to a string in lower case"
        },
        "changeCase.upper": {
          "type": "boolean",
          "default": true,
          "description": "Convert to a string in upper case"
        },
        "changeCase.no": {
          "type": "boolean",
          "default": false,
          "description": "Convert the string without any casing (lower case, space separated)"
        },
        "changeCase.path": {
          "type": "boolean",
          "default": false,
          "description": "Convert to a lower case, slash separated string"
        },
        "changeCase.dot": {
          "type": "boolean",
          "default": false,
          "description": "Convert to a lower case, period separated string"
        },
        "changeCase.lowerFirst": {
          "type": "boolean",
          "default": false,
          "description": "Convert to a string with the first character lower cased"
        },
        "changeCase.upperFirst": {
          "type": "boolean",
          "default": false,
          "description": "Convert to a string with the first character upper cased"
        },
        "changeCase.param": {
          "type": "boolean",
          "default": false,
          "description": "Convert to a lower case, dash separated string"
        },
        "changeCase.sentence": {
          "type": "boolean",
          "default": false,
          "description": "Convert to a lower case, space separated string"
        },
        "changeCase.swap": {
          "type": "boolean",
          "default": false,
          "description": "Convert to a string with every character case reversed"
        },
        "changeCase.title": {
          "type": "boolean",
          "default": true,
          "description": "Convert to a space separated string with the first character of every word upper cased"
        },
        "changeCase.constant": {
          "type": "boolean",
          "default": true,
          "description": "Convert to an upper case, underscore separated string"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^20.19.22",
    "mocha": "^2.3.3",
    "typescript": "^5.9.3",
    "vscode": "^1.0.3"
  },
  "dependencies": {
    "change-case": "^3.0.0",
    "lodash.range": "^3.1.1",
    "lodash.uniq": "^4.1.0"
  }
}
